type Query {
    """
    Simple query used to verify that the canary is responding with the msg.
    """
    msg: String

    """
    Echoes request string back to user.
    """
    echo(msg: String!): String
}

type Subscription {
    """
    Emits an incrementing counter every `intervalMs` milliseconds.
    """
    tick(intervalMs: Int = 1000): Int
}
